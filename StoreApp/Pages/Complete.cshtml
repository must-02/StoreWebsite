@page "/complete"

<div class="text-center">
    <h6 class="display-6">THANKS</h6>
    <p>Thanks for placing order #@OrderId</p>
    <p>We'll ship your orders as soon as possible</p>
    <a class="btn btn-primary" asp-action="Index" asp-controller="Home">Return to Home</a>
</div>

@functions {
    [BindProperty(SupportsGet = true)]
    public String? OrderId { get; set; }
}